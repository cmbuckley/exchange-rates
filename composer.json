{
    "name": "cmbuckley/exchange-rates",
    "description": "A package for interacting with the exchangerate.host API",
    "type": "library",
    "version": "2.0.0",
    "homepage": "https://github.com/cmbuckley/exchange-rates",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StarSquare\\ExchangeRates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarSquare\\ExchangeRates\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Chris Buckley",
            "email": "chris@cmbuckley.co.uk.co.uk"
        },
        {
            "name": "Ash Allen",
            "email": "mail@ashallendesign.co.uk"
        }
    ],
    "keywords": [
        "exchange rate",
        "exchangerate",
        "exchangerate.host"
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nesbot/carbon": "^3.0",
        "phpunit/phpunit": "^11.0",
        "symfony/var-dumper": "^5.3"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
